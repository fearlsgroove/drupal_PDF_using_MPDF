<?php

/**
 * Implement Hook_schema()
 */

function mpdf_uninsstall()
{
	variable_del('mpdf_library_exist');
	variable_set( 'pdf_link_page');
	variable_set( 'pdf_page_size');
	variable_set( 'pdf_font_family');
	variable_set( 'pdf_font_size');
	variable_set( 'pdf_filename');
	variable_set( 'pdf_header_left');
	variable_set( 'pdf_header_center');
	variable_set( 'pdf_header_right');
	variable_set( 'pdf_footer_left');
	variable_set( 'pdf_footer_center');
	variable_set( 'pdf_footer_right');
}

